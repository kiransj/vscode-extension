{
  "name": "cuda-driver-development",
  "displayName": "cuda-driver-development",
  "description": "Setup up environemnt for cuda driver build and debug",
  "version": "0.0.1",
  "publisher": "ksj",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.cudaInit"
  ],
  "main": "./out/src/extension",
