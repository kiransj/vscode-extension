{
  "name": "cuda-driver-development",
  "displayName": "cuda-driver-development",
  "description": "Setup up environemnt for cuda driver build and debug",
  "version": "0.0.2",
  "publisher": "ksj",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.cudaInit",
        "title": "cuda Init"
      },
      {
        "command": "extension.setDebug",
        "title": "cuda set debug"
      },
      {
        "command": "extension.setRelease",
        "title": "cuda set release"
      },
      {
        "command": "extension.setInternal",
        "title": "cuda set internal"
      },
      {
        "command": "extension.setExternal",
        "title": "cuda set external"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "serializer.ts": "0.0.12"
  }
}